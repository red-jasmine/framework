{
    "name": "red-jasmine/invitation",
    "description": "红茉莉邀请领域包，提供邀请码和邀请链接的完整功能",
    "keywords": [
        "invitation",
        "invitation-code",
        "laravel",
        "red-jasmine"
    ],
    "homepage": "https://github.com/red-jasmine/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "liushoukun",
            "email": "liushoukun66@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "red-jasmine/support": "^1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "RedJasmine\\Invitation\\": "src/",
            "RedJasmine\\Invitation\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {
      
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedJasmine\\Invitation\\InvitationPackageServiceProvider",
                "RedJasmine\\Invitation\\Domain\\InvitationDomainServiceProvider",
                "RedJasmine\\Invitation\\Application\\InvitationApplicationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 