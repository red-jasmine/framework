{
    "name": "red-jasmine/shopping",
    "description": "red-jasmine shopping",
    "license": "MIT",
    "require": {
        "red-jasmine/support": "1.0.x-dev",
        "red-jasmine/order": "1.0.x-dev",
        "red-jasmine/address": "1.0.x-dev",
        "red-jasmine/product": "1.0.x-dev",
        "red-jasmine/shop": "1.0.x-dev",
        "red-jasmine/payment": "1.0.x-dev",
        "red-jasmine/coupon": "1.0.x-dev",
        "ext-bcmath": "*"
    },
    "autoload": {
        "psr-4": {
            "RedJasmine\\Shopping\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedJasmine\\Shopping\\ShoppingPackageServiceProvider",
                "RedJasmine\\Shopping\\Application\\ShoppingApplicationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
