{
    "name": "red-jasmine/shop",
    "description": "Shop domain package for Red Jasmine Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Red Jasmine Team",
            "email": "team@red-jasmine.com"
        }
    ],
    "require": {
        "red-jasmine/user": "^1.0",
        "red-jasmine/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RedJasmine\\Shop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedJasmine\\Shop\\ShopPackageServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 