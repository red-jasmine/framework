{
    "name": "red-jasmine/coupon",
    "description": "Red Jasmine Framework - 优惠券领域包",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Red Jasmine Team",
            "email": "team@red-jasmine.com"
        }
    ],
    "require": {
        "red-jasmine/support": "1.0.x-dev"
    },
    "require-dev": {
     
    },
    "autoload": {
        "psr-4": {
            "RedJasmine\\Coupon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedJasmine\\Coupon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "RedJasmine\\Coupon\\CouponPackageServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 