{
    "name": "red-jasmine/coupon",
    "description": "Red Jasmine Framework - 优惠券领域包",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Red Jasmine Team",
            "email": "team@red-jasmine.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "red-jasmine/support": "^1.0",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-data": "^3.0",
        "spatie/query-builder": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RedJasmine\\Coupon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedJasmine\\Coupon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedJasmine\\Coupon\\CouponServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 