{
    "name": "red-jasmine/message",
    "description": "Red Jasmine Message Domain Package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RedJasmine\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedJasmine\\Message\\Tests\\": "tests/"
        }
    },
    "require": {
        "red-jasmine/support": "*",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedJasmine\\Message\\MessagePackageServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
